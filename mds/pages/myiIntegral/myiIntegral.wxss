.container > view.header {
  height: 300rpx;
  position: relative;
  background: linear-gradient(#aad351,#8dba29);
  text-align: center;
}

.container > view.header > .shareBtn {
  position: absolute;
  width: 44rpx;
  height: 40rpx;
  top: 26rpx;
  right: 26rpx;
  border: none;
  outline: none;
  padding: 0;
  background-image: url(data:image/png;base64,);
  background-size: 44rpx 40rpx;
  background-color: transparent;
}

.container > view.header text:first-child {
  padding-top: 45rpx;
}

.container > view {
  padding-bottom: 30rpx;
  border-bottom:1rpx solid #e6e6e6; 
}

.container > view:last-child,
.container > view:first-child {
  border-bottom: none;
  padding-bottom:0;
}

.container > view > .tit {
  background-repeat: no-repeat;
  background-position: 0 23rpx;
  background-size: 29rpx 30rpx;
  padding-left: 40rpx;
}

.header text {
  display: block;
  font-size: 28rpx;
  color: #fff;
}

.header .integral {
  height: 130rpx;
  line-height: 130rpx;
  font-weight: 700;
  font-size: 75rpx;
}

.header .btn {
  padding: 0;
  width: 150rpx;
  height: 35rpx;
  line-height: 35rpx;
  font-size: 20rpx;
  color: #fff;
  font-weight: 100;
  background-color: #e64240;
}

.header .btn image {
  width: 20rpx;
  height: 22rpx;
  margin-top: 2rpx;
  margin-left: 5rpx;
  vertical-align: top;
}

.integral-exchange,
.integral-source {
  width: calc(100% - 102rpx);
  padding: 0 51rpx;
}

.integral-exchange .tit,
.integral-source .tit {
  background-image: 
  url(data:image/png;base64,);
  line-height: 80rpx;
  font-size: 30rpx;
}

.integral-exchange .inner-box {
  position: relative;
  width: 100%;
  height: 150rpx;
  font-size: 24rpx;
  /* overflow: hidden; */
  text-align: center;
}

.integral-exchange .inner-box .inner-item {
  background-image: url('data:image/png;base64,');
  background-size: 100% 150rpx;
  width: 206rpx;
  display: inline-block;  
  height: 150rpx;
  margin-left: 15rpx;
  color: #fff;
}

.inner-inbox {
  white-space: nowrap;
}

.inner-box .inner-item text {
  display: block;
}

.inner-box .arrow-left {
  position: absolute;
  left: -20rpx;
  top: 65rpx;
  width: 20rpx;
  height: 20rpx;
  border-left:#888 1rpx solid;
  border-bottom:#888 1rpx solid;
  transform:rotate(45deg);
}

.inner-box .arrow-right {
  position: absolute;
  right: -20rpx;
  top: 65rpx;
  width: 20rpx;
  height: 20rpx;
  border-right:#888 1rpx solid;
  border-top:#888 1rpx solid;
  transform:rotate(45deg);
}

.inner-box .arrow-left.disable,
.inner-box .arrow-right.disable {
  display: none;
}


.integral-exchange .inner-box .inner-item text:first-child {
  padding-top:45rpx;
}

.integral-exchange .inner-box .inner-item:first-child {
  margin-left: 0;
}

.signIn {
  width: 100%;
  position: relative;
}

.signIn .inner-box {
  width: calc(100% - 52rpx);
  padding: 0 26rpx;
  display: flex;
}

.signIn .inner-box .inner-item {
  flex: 1;
  position: relative;
  font-size: 20rpx;
  text-align: center;
  color: #8dba29;
}

.signIn .inner-box .inner-item text {
  line-height: 75rpx;
}

.signIn .inner-box .inner-item .dot {
  position: absolute;
  z-index: 10;
  top: 77rpx;
  left: 50%;
  transform: translateX(-50%);
  width: 15rpx;
  height: 15rpx;
  border-radius: 50%;
  background-color: #fff;
  border: 5rpx solid #8dba29;
}

.signIn .inner-box .inner-item:last-child .dot,
.inner-box .inner-item .dot image {
  top: 65rpx;
  width: 50rpx;
  height: 50rpx;
  background-color: transparent;
  border: none;
}

.signIn .inner-box .inner-item .icon {
  background-image: url(data:image/png;base64,);
  position: absolute;
  top: 0;
  left: 0;
  height: 75rpx;
  width: 100%;
  background-size: 42rpx 48rpx;
  background-repeat: no-repeat;
  background-position: center center;
  background-color: #fff;
  line-height: 65rpx;
  color: #fff;
}

.signIn .inner-box .inner-item.active .dot {
  background-color: #8dba29;
  border: 5rpx solid #fff;
}

.signIn .inner-box .inner-item.active .icon {
  display: none;
}

.signIn .inner-box .inner-item text:first-child {
  margin-bottom: 25rpx;
}

.signIn .step {
  position: absolute;
  left: 72rpx;
  top: 85rpx;
  background-color: #8dba29;
  height: 5rpx;
  width: calc(100% - 144rpx);
}

.signIn .btn {
  margin: 30rpx auto 0;
  width: 335rpx;
  line-height: 70rpx;
  background-color: #8dba29;
  color: #fff;
  border: none;
  font-size: 30rpx;
  font-weight: 100;
  border-radius: 40rpx;
}

.signIn .btn.disable {
  background-color: #e6e6e6;
  color: #888;
}

.container > .sign-dialog {
  z-index: 10;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  border: none;
  width: 640rpx;
  height: 480rpx;
  padding-bottom: 0;
  background-color: rgba(0,0,0,0.7); 
  box-shadow: 0 0 0 750rpx rgba(0,0,0,0.7);
}

 .container > .sign-dialog .inner {
  margin: 0 auto;
  text-align: center;
  position: relative;
  top: 260rpx;
  width: 50%;
  height: 180rpx;
} 

.sign-dialog .inner text {
  display: block;
  font-size: 26rpx;
  line-height: 50rpx;
}

.sign-dialog .inner .btn {
  margin-top:10rpx;
  font-size: 30rpx;
  padding: 0;
  background-color: #8dba29;
  color: #fff;
}

.sign-dialog > .close {
  padding: 0;
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  bottom: -70rpx;
  border-radius: 50%;
  background-color: transparent;
  width: 44rpx;
  height: 44rpx;
  border: 1rpx solid #fff;
}

.sign-dialog > .close:after {
  content: '\2715';
  font-weight: 100;
  position: absolute;
  left: 50%;
  top: 0;
  line-height: 40rpx;
  font-size: 30rpx;
  transform: translateX(-50%);
  color: #fff;
}

.sign-dialog image {
  position: absolute;
}

.integral-source .tit{
  background-image: url('data:image/png;base64,');
}

.integral-source .inner-box {
  font-size: 24rpx;
  padding: 0 30rpx;
}

.integral-source .inner-box text {
  display: block;
  margin-bottom: 10rpx;
}

