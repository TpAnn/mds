.header{
  width: 100%;
  overflow: hidden;
  text-align: center;
  height: 400rpx;
  color: #fff;
  letter-spacing: 2rpx;
}
.bottomBox{
  border:#fff 1px solid;
  border-radius: 20rpx;
  height: 28rpx;
  width: 200rpx;
  line-height: 25rpx;
  margin:0 auto;

}
.bottomBox text{
  font-size:12rpx ;
}
.header view{
  font-size:28rpx;
  line-height:40rpx;
}
.header-top{
  width: 150rpx;
  height: 150rpx;
  background: #fff;
  border-radius: 50%;
  margin:0 auto;  
  margin-top:50rpx;
  border:#d7e8b2 5px solid;
}
.header .btn {
  padding: 0;
  width: 150rpx;
  height: 35rpx;
  margin-top: 10rpx;
  line-height: 35rpx;
  font-size: 20rpx;
  color: #fff;
  font-weight: 100;
  background-color: #e64240;
}

.header .btn image {
  width: 20rpx;
  height: 22rpx;
  margin-top: 2rpx;
  margin-left: 5rpx;
  vertical-align: top;
}

.header-top + view {
  margin-top: 15rpx;
}
.red{
  color: red;
}
.navTitle{
  text-align: center;
  margin-top:35rpx;
  font-weight: bold
}
.box{
  width: 100%;
  text-align: center;
  /* display: none; */
}
.top{
  overflow: hidden;
   width: 520rpx;
   margin: 0 auto;
   margin-bottom: 45rpx;
}
.top view{
  float: left;
  width: 120rpx;
  height: 136rpx;
  margin-top: 35rpx;
  font-size: 28rpx;
  text-align:center;
}
.top image{
  width: 94rpx;
  height: 94rpx;
  border-radius: 50%;
  margin: 0 12rpx;
}
.top text{
  margin-bottom: 10rpx;
  font-size: 27rpx;
  color: #343434;
}
.box-list{
  width: calc(100%-52rpx);
  border:#dcdcdc 1rpx solid;
  border-radius: 20rpx;
  margin: 0 26rpx;
  padding-right: 26rpx;
  margin-bottom:20rpx;
}
.box-list.curr {
  border: 1rpx solid #8dba29;
  background-color: #f4f8e9;
}
.lsitT{
  margin: 40rpx 0 0 30rpx;
  font-size: 30rpx;
  overflow: hidden;
}
.listB{
  font-size: 20rpx;
  color: #888;
  margin:0 0 40rpx 30rpx;
  overflow: hidden;
}
.lsitT text {
  float: right;
  line-height: 50rpx;
  font-size: 40rpx;
  font-weight: 700;
}
.lsitT text:first-child {
  float: left;
}
.lsitT text:last-child {
  font-size: 26rpx;
  font-weight: 400;
}
.listB text:last-child {
  float: right;
}
.listB text:last-child{
  text-decoration: line-through;
}

.lsitT text text{
  font-size: 30rpx;
}
page{
  font-size: 30rpx;
}

.buttom > .btn{
  width: 100%;
  height: 98rpx;
  background-color: #8cba28;
  font-size: 30rpx;
  position: fixed;
  bottom: 0rpx;
  font-weight: bold;
  letter-spacing: 2rpx;
  line-height: 98rpx;
  text-align: center;
  color: #fff;
}
.zf {
  padding-left: 26rpx;
  font-size: 30rpx;
  color: #010101;
  line-height: 114rpx;
  height: 114rpx;
}
.wx{
  width: 100%;
  line-height: 100rpx;
  height: 100rpx;
  border-top:#e7e7e7 1rpx solid;
  border-bottom:#e7e7e7 1rpx solid;
  margin-bottom: 100rpx;
  position: relative;
}
.wx image ,.wx view{
  float: left;
  line-height: 100rpx;
}
.wx image:first-child{
  margin: 25rpx 10rpx 0 26rpx;
  width:57rpx; height:57rpx; 
  border-radius: 50%;
}
.item-select {
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  right: 26rpx;
  top:35rpx; 
}
.backgroundgary{
  position: fixed;
  z-index: 11;
  background-color:rgba(000,000,000,0.3);
  width: 100%;
  height: 100%;
}
.box{
  background-color: #fff;
  width: 530rpx;
  border-radius: 20rpx;
  margin-top: 50%;
  margin-left: 50%;
  text-align: center;
  padding-bottom: 125rpx;
  transform: translateX(-50%);
}
.title{
  font-size: 30rpx;
  color: #000000;
  font-weight: bold;
  padding-top:25rpx;
  padding-bottom: 30rpx;
}
.list {
  width: 100%;
  height: 50rpx;
  line-height:50rpx;
  font-size: 26rpx;
}
.list .list-item {
  display: inline-block;
  position: relative;
  width: 250rpx;
  margin-left:40rpx;
  text-align: left;
}
.list .list-item.w200 {
  width: 200rpx;
}
.list .list-item:after {
  content: '';
  position: absolute;
  left: -32rpx;
  height: 12rpx;
  width: 12rpx;
  top: 50%;
  border-radius: 50%;
  background-color: #8dba29;
  transform: translateY(-50%);
}
.backgroundgary .box .btn {
  width:350rpx;
  height:65rpx;
  line-height:65rpx;
  background-color:#8dba29;
  color:#fff;
  border-radius:40rpx;
  margin-bottom: 25rpx;
  margin-left: 50%;
  transform: translateX(-50%);
}