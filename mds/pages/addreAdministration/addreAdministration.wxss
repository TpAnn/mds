page{
  background-color:#f5f5f5;
  padding-bottom:100rpx;
}
.header{
  width: calc( 100% - 60rpx );
  height: 122rpx;
  background-color: #fff;
  position: relative;
  margin-top: 20rpx;
  padding:0 30rpx;
}
.header:last-child {
  margin-bottom: 87rpx;
}
.header:first-child {
  margin-top: 0;
}
.header image{
  width: 49rpx;
  height: 49rpx;
  position: absolute;
  right: 26rpx;
  bottom: 20rpx;
}
.header.defaultAddr .mr {
  display: inline;
}
.mr{
  display: none;
  background-color: #e64340;
  font-size: 28rpx;
  color: #fff;
  padding:1rpx 15rpx;
  border-radius: 8rpx;
  margin-right: 10rpx;
}
.headertop{
  overflow: hidden;
  padding-top:10rpx;
  line-height: 50rpx;
}
.headertop view{
  font-size: 28rpx;
  float: left
}
.headertop view:last-child{
  color: #888888;
  position: absolute;
  right: 100rpx;
}
.headerbottom {
  font-size: 28rpx;
  line-height: 60rpx;
  border-bottom: #e2e0e0 1rpx solid;
  width: 100%;
}
.conentList{
  background-color:#fff;
  color: #353535;
  line-height: 70rpx;
  overflow: hidden;
  height:70rpx;
  font-size: 30rpx;
}
.conentList image{
  width: 40rpx;
  height: 40rpx;
  margin: 16rpx;
  margin-left: 30rpx;
  float:left;
}

.bnt{
  width: 100%;
  height: 87rpx;
  line-height: 87rpx;
  color: #fff;
  background-color: #8cba28;
  text-align: center;
  position: fixed;
  bottom: 0rpx;
}